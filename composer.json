{
    "name": "pochocho/okta-saml-sso",
    "description": "SSO for Laravel with Okta",
    "require": {
        "illuminate/support": "^9.0",
        "litesaml/lightsaml": "^4.0",
        "illuminate/auth": "^9.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pochocho\\OktaSamlSso\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pochocho\\OktaSamlSso\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "pochocho",
            "email": "ccarretta@pcubelabs.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Pochocho\\OktaSamlSso\\OktaSamlSsoServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.15"
    }
}
